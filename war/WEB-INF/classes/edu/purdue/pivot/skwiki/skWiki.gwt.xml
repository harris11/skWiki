<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module rename-to="skWiki">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.activity.Activity" />
	<!-- <inherits name="com.googlecode.mgwt.MGWT"/> -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<inherits name="com.google.gwt.resources.Resources" />
	<!-- <inherits name='com.emitrom.easygwt.wf.EasyGWT'/> -->
    <!-- <inherits name="com.google.gwt.logging.Logging"/> -->
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name='gwt.g2d.g2d' />
	<inherits name="gwtupload.GWTUpload" />
	<stylesheet src="../upload.css" />
	<stylesheet src="../dnd.css" />
	<inherits name="com.mogaleaf.SimpleColorPicker" />
	<inherits name="com.kiouri.sliderbar.SliderBarMVP" />
	<inherits name='org.vaadin.gwtgraphics.GWTGraphics' />
	<inherits name="com.smartgwt.SmartGwt" />
	
	<!-- set the user agent to safari -> only webkit -->
	<set-property name="user.agent" value="gecko1_8, safari" />
	<inherits name='com.google.code.gwt.crop.GWTCropper' />

	<!-- 	 <set-property name="mgwt.os" value="android" /> -->
	<!-- 	<set-configuration-property name="mgwt.css" value="pretty" /> -->

	<source path="client" />
	<source path='shared' />
	<entry-point class="edu.purdue.pivot.skwiki.client.SkwikiEntryPoint" />
	<!-- <entry-point class="edu.purdue.pivot.skwiki.client.SkwikiLoginPage" /> -->
</module>